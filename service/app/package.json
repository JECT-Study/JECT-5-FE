    "react": "^18",
    "react-dom": "^18",
    "react-simplikit": "^0.0.40",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@ject-5-fe/eslint": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4",
    "@vitest/browser": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "msw": "^2.10.3",
