{
  "name": "@ject-5-fe/app",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn run -T -B eslint --max-warnings 0 --no-warn-ignored --fix",
    "type-check": "tsc --noEmit",
